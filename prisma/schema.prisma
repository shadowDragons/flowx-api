generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @default(autoincrement()) @id
  name  String @unique
  password String
}

model Project {
  id        Int      @default(autoincrement()) @id
  type      Int
  title     String
  description String
  status    Int
}
